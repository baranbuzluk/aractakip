<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="rootPane" maxHeight="1.7976931348623157E308"
	maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox>
			<children>
				<GridPane VBox.vgrow="NEVER">
					<children>
						<TextField fx:id="fieldName" GridPane.columnIndex="1">
							<padding>
								<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
							</padding>
						</TextField>
						<ComboBox fx:id="cboxCategory" GridPane.columnIndex="1"
							GridPane.rowIndex="1" />
						<ComboBox fx:id="cboxColor" GridPane.columnIndex="3" />
						<ComboBox fx:id="cboxSize" GridPane.columnIndex="3"
							GridPane.rowIndex="1" />
						<TextField fx:id="fieldCreditPrice"
							GridPane.columnIndex="5" />
						<TextField fx:id="fieldCashPrice"
							GridPane.columnIndex="5" GridPane.rowIndex="1" />
						<TextField fx:id="fieldId" disable="true"
							GridPane.columnIndex="7">
							<padding>
								<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
							</padding>
						</TextField>
						<ComboBox fx:id="cboxYear" GridPane.columnIndex="7"
							GridPane.rowIndex="1" />
						<Label text="Ürün İsmi:" GridPane.halignment="RIGHT">
							<GridPane.margin>
								<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
							</GridPane.margin>
						</Label>
						<Label text="Kategori:" GridPane.halignment="RIGHT"
							GridPane.rowIndex="1">
							<GridPane.margin>
								<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
							</GridPane.margin>
						</Label>
						<Label text="Renk:" GridPane.columnIndex="2"
							GridPane.halignment="RIGHT">
							<GridPane.margin>
								<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
							</GridPane.margin>
						</Label>
						<Label text="Boyut:" GridPane.columnIndex="2"
							GridPane.halignment="RIGHT" GridPane.rowIndex="1">
							<GridPane.margin>
								<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
							</GridPane.margin>
						</Label>
						<Label text="Nakit Fiyat:" GridPane.columnIndex="4"
							GridPane.halignment="RIGHT" GridPane.rowIndex="1">
							<GridPane.margin>
								<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
							</GridPane.margin>
						</Label>
						<Label text="Vadeli Fiyat:" GridPane.columnIndex="4"
							GridPane.halignment="RIGHT">
							<GridPane.margin>
								<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
							</GridPane.margin>
						</Label>
						<Label text="Ürün ID:" GridPane.columnIndex="6"
							GridPane.halignment="RIGHT">
							<GridPane.margin>
								<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
							</GridPane.margin>
						</Label>
						<Label text="Yıl:" GridPane.columnIndex="6"
							GridPane.halignment="RIGHT" GridPane.rowIndex="1">
							<GridPane.margin>
								<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
							</GridPane.margin>
						</Label>
					</children>
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="1.7976931348623157E308" />
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="1.7976931348623157E308" />
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="1.7976931348623157E308" />
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="1.7976931348623157E308" />
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="1.7976931348623157E308" />
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="1.7976931348623157E308" />
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="1.7976931348623157E308" />
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="1.7976931348623157E308" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints maxHeight="1.7976931348623157E308"
							percentHeight="0.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="1.7976931348623157E308"
							percentHeight="0.0" vgrow="SOMETIMES" />
						<RowConstraints maxHeight="1.7976931348623157E308"
							percentHeight="0.0" vgrow="SOMETIMES" />
					</rowConstraints>
					<VBox.margin>
						<Insets bottom="5.0" right="20.0" top="10.0" />
					</VBox.margin>
				</GridPane>
				<HBox alignment="CENTER" fillHeight="false" VBox.vgrow="NEVER">
					<children>
						<Button fx:id="btnSave" alignment="CENTER"
							contentDisplay="CENTER" maxHeight="-Infinity"
							maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
							mnemonicParsing="false" prefHeight="20.0" prefWidth="75.0"
							text="Kaydet" textOverrun="CENTER_ELLIPSIS">
							<font>
								<Font name="Calibri Bold" size="10.0" />
							</font>
							<HBox.margin>
								<Insets left="10.0" right="10.0" />
							</HBox.margin>
						</Button>
						<Button fx:id="btnSearch" alignment="CENTER"
							contentDisplay="CENTER" maxHeight="-Infinity"
							maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
							mnemonicParsing="false" prefHeight="20.0" prefWidth="75.0"
							text="Ara" textOverrun="CENTER_ELLIPSIS">
							<font>
								<Font name="Calibri Bold" size="10.0" />
							</font>
							<HBox.margin>
								<Insets left="10.0" right="10.0" />
							</HBox.margin>
						</Button>
						<Button fx:id="btnRefresh" alignment="CENTER"
							contentDisplay="CENTER" maxHeight="-Infinity"
							maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
							mnemonicParsing="false" prefHeight="20.0" prefWidth="75.0"
							text="Yenile" textOverrun="CENTER_ELLIPSIS">
							<font>
								<Font name="Calibri Bold" size="10.0" />
							</font>
							<HBox.margin>
								<Insets left="10.0" right="10.0" />
							</HBox.margin>
						</Button>
					</children>
					<VBox.margin>
						<Insets bottom="10.0" top="10.0" />
					</VBox.margin>
				</HBox>
				<TableView fx:id="tableProductEntities"
					nodeOrientation="LEFT_TO_RIGHT" VBox.vgrow="ALWAYS">
					<columnResizePolicy>
						<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
					</columnResizePolicy>
					<columns>
						<TableColumn fx:id="columnId" editable="false"
							maxWidth="200.0" minWidth="-1.0" prefWidth="-1.0"
							sortable="false" style="-fx-alignment: center;" text="Ürün ID" />
						<TableColumn fx:id="columnName" editable="false"
							maxWidth="300.0" minWidth="-1.0" prefWidth="-1.0"
							sortable="false" style="-fx-alignment: center;" text="Ürün İsim" />
						<TableColumn fx:id="columnCategory" editable="false"
							maxWidth="200.0" minWidth="-1.0" prefWidth="-1.0"
							sortable="false" style="-fx-alignment: center;" text="Kategori" />
						<TableColumn fx:id="columnColor" editable="false"
							maxWidth="200.0" minWidth="-1.0" prefWidth="-1.0"
							sortable="false" style="-fx-alignment: center;" text="Renk" />
						<TableColumn fx:id="columnSize" editable="false"
							maxWidth="200.0" minWidth="-1.0" prefWidth="-1.0"
							sortable="false" style="-fx-alignment: center;" text="Boyut" />
						<TableColumn fx:id="columnCreditPrice"
							editable="false" maxWidth="200.0" minWidth="-1.0"
							prefWidth="-1.0" sortable="false" style="-fx-alignment: center;"
							text="Vadeli Fiyat" />
						<TableColumn fx:id="columnCashPrice" editable="false"
							maxWidth="200.0" minWidth="-1.0" prefWidth="-1.0"
							sortable="false" style="-fx-alignment: center;"
							text="Nakit Fiyat" />
						<TableColumn fx:id="columnYear" editable="false"
							maxWidth="200.0" minWidth="-1.0" prefWidth="-1.0"
							sortable="false" style="-fx-alignment: center;" text="Yıl" />
					</columns>
				</TableView>
			</children>
		</VBox>
	</children>
</StackPane>
